{
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.codeActionsOnSave": {
        "source.fixAll": "always"
    },
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar",
        "editor.formatOnSave": false,
    },
    "eslint.validate": [
        "vue",
        "typescript"
    ],
}